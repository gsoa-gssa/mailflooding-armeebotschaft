models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    Contact: { softdeletes: softDeletes, firstname: string, lastname: string, email: string, phone: 'string nullable', optin: 'boolean default:true' }
    Email: { softdeletes: softDeletes, subject: string, body: text, sent_at: 'datetime nullable', contact_id: unsignedInteger, relationships: { belongsTo: Contact } }
    Politician: { softdeletes: softDeletes, name: string, email: string, image: 'string nullable', canton_id: unsignedInteger, faction_id: unsignedInteger, council_id: 'unsignedInteger nullable', relationships: { belongsTo: 'Canton, Faction, Council' } }
    Canton: { softdeletes: softDeletes, name: string, relationships: { hasMany: Politician } }
    Faction: { softdeletes: softDeletes, name: string, relationships: { hasMany: Politician } }
    Council: { softdeletes: softDeletes, name: string, relationships: { hasMany: Politician } }
